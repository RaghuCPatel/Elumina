{
  "name": "playwright-typescript-playwright-test",
  "version": "1.0.0",
  "description": "Playwright Framework with Typescript",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:single": "npx playwright test Api_Blueprint_And_Image_Service.test.ts --project=API",
    "test:elumina": "npx playwright test Elumina.test.ts --project=Chrome",
    "test:parallel": "npx playwright test --grep @Smoke --project=Chrome",
    "test:serial": "npx playwright test --grep @LowPriorityiExamCases --workers=1 --project=Chrome",
    "test:smoke_Serial": "npx playwright test --grep \"@Smoke|@iProctorlink\" --workers=1 --project=Chrome",
    "test:iExam_Parallel_Prerequest_Regression": "npx playwright test --grep @Pre-Request --workers=4 --project=Chrome",
    "test:iExam_Parallel_Regression": "npx playwright test --grep \"@iExamRegression|@LowPriorityiExamCases\" --workers=4 --project=Chrome",
    "test:iExam_Serial_Regression": "npx playwright test --grep \"@Serial-Pre-Request|@iExamSerialRegression\" --workers=1 --project=Chrome",
    "test:iProctor_Serial_Regression": "npx playwright test --grep \"@iProctorRegression|@LowPriorityiProctorCases\" --workers=1 --project=Chrome",
    "test:iAuthor_Parallel_Regession": "npx playwright test --grep @RegressionA1 --workers=2 --project=Chrome",
    "test:cicd-regression": "npx playwright test --grep \"@iExamRegression|@LowPriorityiExamCases\" --workers=1 --project=Chromium",
    "test:cicd-smoke": "npx playwright test --grep \"@Smoke|@iProctorlink\" --workers=1 --project=Chromium",
    "test:smoke_Serial_CICD": "npx playwright test --grep \"@Smoke|@iProctorlink\" --workers=1 --project=Chrome",
    "test:iExam_Parallel_run_Prerequest_Regression_CICD": "npx playwright test --grep @Pre-Request --workers=4 --project=Chrome",
    "test:iExam_Parallel_Regression_CICD": "npx playwright test --grep \"@iExamRegression|@LowPriorityiExamCases\" --workers=4 --project=Chrome",
    "test:iExam_Serial_Regression_CICD": "npx playwright test --grep \"@Serial-Pre-Request|@iExamSerialRegression\" --workers=1 --project=Chrome",
    "test:iProctor_Serial_Regression_CICD": "npx playwright test --grep \"@iProctorRegression|@LowPriorityiProctorCases\" --workers=1 --project=Chrome",
    "test:iAuthor_Parallel_Regession_CICD": "npx playwright test --grep @RegressionA --workers=4 --project=Chrome",
    "test:iAuthor_Parallel_Zephyr_CICD": "npx playwright test --grep @Zephyr --workers=2 --project=Chrome",
    "test:ui": "npx playwright test --grep @Smoke --ui",
    "test:api": "npx playwright test --grep @API --workers=1 --project=API",
    "test:db": "npx playwright test DB.test.ts --project=DB",
    "test:record": "npx playwright codegen",
    "test:visual": "npx playwright test visualComparision.test.ts --project=Chrome",
    "test:device": "npx playwright test Emulation.test.ts --project=Device",
    "allureReport": "allure generate ./allure-results && allure open",
    "lighthouse": "node tests/lighthouse/Lighthouse.js",
    "clean": "rimraf allure-report/ && rimraf allure-results && rimraf test-results"
  },
  "author": "IGS",
  "license": "MIT",
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/crypto-js": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "adm-zip": "^0.5.10",
    "ajv": "^8.12.0",
    "allure-playwright": "^2.1.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "email-generator": "^1.0.1",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "exceljs": "^4.3.0",
    "i": "^0.3.7",
    "lighthouse": "^9.6.8",
    "npm": "^9.6.4",
    "pdfjs-dist-es5": "^2.13.216",
    "pg": "^8.10.0",
    "typescript": "^5.0.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "playwright-zephyr": "^1.0.3"
  }
}